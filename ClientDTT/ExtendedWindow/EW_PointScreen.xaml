<Window      x:Class="ClientDTT.ExtendedWindow.EW_PointScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ClientDTT.ExtendedWindow"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="DarkBlue">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="200*"/>
            <RowDefinition Height="250*"/>
        </Grid.RowDefinitions>
        <Viewbox Grid.Row="0">
            <TextBlock Name="txtBlockMainStudent" Text="TS" Foreground="White" FontWeight="DemiBold"/>
        </Viewbox>
        <Viewbox Grid.Row="1">
            <TextBlock Name="txtBlockMainPoint" Text="0" Foreground="White" FontWeight="DemiBold"/>
        </Viewbox>
    </Grid>
</Window>
